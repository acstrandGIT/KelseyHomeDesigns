@{
    ViewBag.Title = "Home Page";
}

<main>
    <div class="cormorant-garamond-mainfont">
        <link rel="stylesheet" href="~/Styles/StyleSheet1.css" />


        <div id="photocarousel" class="carousel slide" data-bs-ride="carousel">
            <ol class="carousel-indicators">
                <li data-bs-target="#photocarousel" data-slide-to="0" class="active"></li>
                <li data-bs-target="#photocarousel" data-slide-to="1"></li>
                <li data-bs-target="#photocarousel" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100" src="~/images/Kitchen.jpg" alt="First slide">
                    <div class="carousel-caption fs-1">
                        <h1>Kelsey's Home Designs</h1>
                        <p>Explore all you need and find your dream home</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/images/LivingRoom1.jpg" alt="Second slide">
                    <div class="carousel-caption fs-1">
                        <h1>Kelsey's Home Designs</h1>
                        <p>Explore all you need and find your dream home</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/images/Bathroom1.jpg" alt="Third slide">
                    <div class="carousel-caption fs-1">
                        <h1>Kelsey's Home Designs</h1>
                        <p>Explore all you need and find your dream home</p>
                    </div>
                </div>
            </div>
            <a class="carousel-control-prev" href="#photocarousel" role="button" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#photocarousel" role="button" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>


        <div class="row mt-6">

            <section class="col-md-4" aria-labelledby="gettingStartedTitle">
                <h2 id="gettingStartedTitle">Getting Started</h2>
                <p>
                    How to get started with us
                </p>
                <p><a class="btn btn-outline-dark" href="https://go.microsoft.com/fwlink/?LinkId=301865">Click to Get Started &raquo;</a></p>
            </section>
            <section class="col-md-4" aria-labelledby="librariesTitle">
                <div class="Logo">
                    <a target="_blank" href="~/images/Kelsey Designs_Logo.jpg">
                        <img src="~/images/Kelsey Designs_Logo.jpg" alt="Logo1" />
                    </a>
                </div>
            </section>
            <section class="col-md-4" aria-labelledby="hostingTitle">
                <h2 id="hostingTitle">ABOUT US</h2>
                <p>At Kelsey's Design, we have fallen in love with creating modern spaces for others that tell their story. A space for our clients to reflect, reset and create. We believe that your space is a reflection of who you are, and that quality design shouldn't have to be expensive. Together, we can help you design a space that is unique to your style, your vision, and your wallet.</p>
                <p><a class="btn btn-outline-dark" href="https://go.microsoft.com/fwlink/?LinkId=301867">Learn About Us &raquo;</a></p>
            </section>
        </div>
    </div>
</main>