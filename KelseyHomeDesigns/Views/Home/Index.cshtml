@{
    ViewBag.Title = "Home Page";
}

<main>
    <div class="cormorant-garamond-mainfont">
        <link rel="stylesheet" href="~/Styles/StyleSheet1.css" />


        <div id="photocarousel" class="carousel slide" data-bs-ride="carousel">
            <ol class="carousel-indicators">
                <li data-bs-target="#photocarousel" data-slide-to="0" class="active"></li>
                <li data-bs-target="#photocarousel" data-slide-to="1"></li>
                <li data-bs-target="#photocarousel" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100" src="~/images/Kitchen.jpg" alt="First slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/images/LivingRoom1.jpg" alt="Second slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/images/Bathroom1.jpg" alt="Third slide">
                </div>
            </div>
            <a class="carousel-control-prev" href="#photocarousel" role="button" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#photocarousel" role="button" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>




        <section class="one" aria-labelledby="aspnetTitle">
            <h1 id="title">Kelsey's Home Designs'</h1>
            <p class="lead">Explore all you need and find your dream home</p>
        </section>


        <div class="row">

            <section class="col-md-4" aria-labelledby="gettingStartedTitle">
                <h2 id="gettingStartedTitle">Getting Started</h2>
                <p>
                    HOW TO GET STARTED WITH US
                </p>
                <p><a class="btn btn-outline-dark" href="https://go.microsoft.com/fwlink/?LinkId=301865">Learn more &raquo;</a></p>
            </section>
            <section class="col-md-4" aria-labelledby="librariesTitle">
                <div class="Logo">
                    <a target="_blank" href="~/images/Kelsey Designs_Logo.jpg">
                        <img src="~/images/Kelsey Designs_Logo.jpg" alt="Logo1" />
                    </a>
                </div>
            </section>
            <section class="col-md-4" aria-labelledby="hostingTitle">
                <h2 id="hostingTitle">ABOUT US</h2>
                <p>You can easily find a web hosting company that offers the right mix of features and price for your applications.</p>
                <p><a class="btn btn-outline-dark" href="https://go.microsoft.com/fwlink/?LinkId=301867">Learn more &raquo;</a></p>
            </section>
        </div>
    </div>
</main>